{
  "name": "@-0/stack",
  "author": "Logan Powell",
  "license": "MIT",
  "version": "0.2.53",
  "description": "SPA framework built on top of http://thi.ng/umbrella ecosystem",
  "main": "./lib/index.js",
  "type": "module",
  "types": "./lib/index.d.ts",
  "repository": "https://github.com/subs0/stack",
  "homepage": "https://github.com/subs0/stack",
  "scripts": {
    "build": "npm run clean && cross-env NODE_ENV=production parcel build example/index.html -d @-0/stack --no-source-maps --no-cache --public-url ./",
    "start": "npm run types && parcel example/index.html --global md -p 8080 --open -d dist watch",
    "clean": "rm -rf .cache build dist",
    "madge": "madge --circular lib/",
    "typewatch": "tsc --project tsconfig.json",
    "types": "tsc --project tsconfig.build.json",
    "jsdoc": "jsdoc -c jsdoc/jsdoc.json --verbose --debug",
    "pages": "npm run build && gh-pages -d @-0/stack",
    "docs": "npm run types && npm run jsdoc",
    "patch": "npm version patch && npm run docs && npm run pages && npm publish",
    "tests": "npm run types && npm run madge && jest",
    "postgit": "git push origin master && npm run patch",
    "git": "git add . && git commit -m",
    "tp:keys": "node scripts/types st=keys",
    "tp:spool": "node scripts/types st=spool",
    "tp:utils": "node scripts/types st=utils",
    "tp:dom": "node scripts/types st=dom",
    "up:keys": "node scripts/update st=keys",
    "up:utils": "node scripts/update st=utils",
    "up:spool": "node scripts/update st=spool",
    "up:dom": "node scripts/update st=dom",
    "pa:keys": "node scripts/patch st=keys",
    "pa:utils": "node scripts/patch st=utils",
    "pa:spool": "node scripts/patch st=spool",
    "pa:dom": "node scripts/patch st=dom",
    "st:keys": "node scripts/subtree st=keys via=--",
    "st:utils": "node scripts/subtree st=utils via=--",
    "st:spool": "node scripts/subtree st=spool via=--",
    "st:dom": "node scripts/subtree st=dom via=",
    "go:keys": "npm run tp:keys && npm run up:keys && npm run st:keys && npm run ncu",
    "go:utils": "npm run tp:utils && npm run up:utils && npm run st:utils && npm run ncu",
    "go:spool": "npm run tp:spool && npm run up:spool && npm run st:spool && npm run ncu",
    "go:dom": "npm run tp:dom && npm run up:dom && npm run st:dom && npm run ncu",
    "go:all": "npm run go:keys && npm run go:utils && npm run go:spool && npm run go:dom",
    "ncu": "ncu -u && npm i"
  },
  "keywords": [
    "thi.ng",
    "multiplex",
    "FRP"
  ],
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "@thi.ng/arrays": "latest",
    "@thi.ng/associative": "latest",
    "@thi.ng/atom": "latest",
    "@thi.ng/checks": "latest",
    "@thi.ng/paths": "latest",
    "@thi.ng/transducers": "latest",
    "@thi.ng/rstream": "^3.0.2",
    "@thi.ng/transducers-hdom": "^2.0.34"
  },
  "dependencies": {
    "@-0/dom": "^0.2.62",
    "@-0/keys": "^0.2.61",
    "@-0/spool": "^0.2.64",
    "@-0/utils": "^0.2.57"
  },
  "devDependencies": {
    "@thi.ng/arrays": "latest",
    "@thi.ng/associative": "latest",
    "@thi.ng/atom": "latest",
    "@thi.ng/checks": "latest",
    "@thi.ng/paths": "latest",
    "@thi.ng/rstream": "^3.0.2",
    "@thi.ng/transducers": "latest",
    "@thi.ng/transducers-hdom": "^2.0.34",
    "@typescript-eslint/eslint-plugin": "^2.19.2",
    "@typescript-eslint/parser": "^2.19.2",
    "better-docs": "^1.4.7",
    "cross-env": "^7.0.0",
    "cssnano": "^4.1.10",
    "eslint": "^6.8.0",
    "gh-pages": "^2.2.0",
    "jest": "^25.1.0",
    "prettier": "^1.19.1"
  },
  "prettier": {
    "singleQuote": false,
    "printWidth": 100,
    "endOfLine": "lf",
    "tabWidth": 2,
    "semi": false
  }
}
