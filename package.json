{
    "name": "@-0/stack",
    "author": "Logan Powell",
    "license": "MIT",
    "version": "0.3.0",
    "description": "Monorepo for -0/ repo subtrees",
    "main": "./lib/index.js",
    "type": "module",
    "types": "./lib/index.d.ts",
    "repository": "https://github.com/subs0/stack",
    "homepage": "https://github.com/subs0/stack",
    "scripts": {
        "madge": "madge --circular lib/",
        "types": "tsc --project tsconfig.build.json",
        "typewatch": "tsc --project tsconfig.json",
        "test": "npm run madge && concurrently \"npm run typewatch\" \"jest --watchAll\"",
        "pages": "npm run build && gh-pages -d @-0/stack",
        "jsdoc": "jsdoc -c jsdoc/jsdoc.json --verbose --debug",
        "docs": "npm run types && npm run jsdoc",
        "clean": "rm -rf .cache build dist",
        "build": "npm run clean && cross-env NODE_ENV=production parcel build example/index.html -d @-0/stack --no-source-maps --no-cache --public-url ./",
        "start": "npm run madge && concurrently \"npm run typewatch\" \"parcel example/index.html md -p 8080 --open -d dist watch\"",
        "postgit": "git push origin master",
        "git": "git add . && git commit -m",
        "cl:keys": "node scripts/clean st=keys",
        "cl:utils": "node scripts/clean st=utils",
        "cl:spool": "node scripts/clean st=spool",
        "cl:browser": "node scripts/clean st=browser",
        "cl:hdom": "node scripts/clean st=hdom",
        "cl:amplify": "node scripts/clean st=amplify",
        "tp:keys": "node scripts/types st=keys",
        "tp:spool": "node scripts/types st=spool",
        "tp:utils": "node scripts/types st=utils",
        "tp:browser": "node scripts/types st=browser",
        "tp:hdom": "node scripts/types st=hdom",
        "tp:amplify": "node scripts/types st=amplify",
        "up:keys": "node scripts/update st=keys",
        "up:utils": "node scripts/update st=utils",
        "up:spool": "node scripts/update st=spool",
        "up:browser": "node scripts/update st=browser",
        "up:hdom": "node scripts/update st=hdom",
        "up:amplify": "node scripts/update st=amplify",
        "st:keys": "node scripts/subtree st=keys via=push",
        "st:utils": "node scripts/subtree st=utils via=push",
        "st:spool": "node scripts/subtree st=spool via=push",
        "st:browser": "node scripts/subtree st=browser via=push",
        "st:hdom": "node scripts/subtree st=hdom via=push",
        "st:amplify": "node scripts/subtree st=amplify via=push",
        "go:keys": "npm run cl:keys && npm run tp:keys && npm run up:keys && npm run st:keys",
        "go:utils": "npm run cl:utils && npm run tp:utils && npm run up:utils && npm run st:utils",
        "go:spool": "npm run cl:spool && npm run tp:spool && npm run up:spool && npm run st:spool",
        "go:browser": "npm run cl:browser && npm run tp:browser && npm run up:browser && npm run st:browser",
        "go:hdom": "npm run cl:hdom && npm run tp:hdom && npm run up:hdom && npm run st:hdom",
        "go:all": "npm run go:keys && npm run go:utils && npm run go:spool && npm run go:browser && npm run ncu",
        "ncu": "ncu -u && npm i && npm audit fix"
    },
    "keywords": [
        "thi.ng",
        "multiplex",
        "FRP"
    ],
    "dependencies": {
        "@-0/browser": "^0.3.19",
        "@-0/hdom": "^0.3.13",
        "@-0/keys": "^0.3.20",
        "@-0/spool": "^0.3.16",
        "@-0/utils": "^0.3.20"
    },
    "peerDependencies": {
        "@thi.ng/paths": "^4.2.12",
        "@thi.ng/rstream": "^6.0.16",
        "@thi.ng/transducers-hdom": "^2.0.101"
    },
    "devDependencies": {
        "@babel/plugin-transform-modules-commonjs": "^7.15.0",
        "@babel/preset-env": "^7.15.0",
        "@testing-library/dom": "^8.1.0",
        "@thi.ng/arrays": "^1.0.1",
        "@thi.ng/associative": "5.2.11",
        "@thi.ng/atom": "^4.1.40",
        "@thi.ng/checks": "^2.9.10",
        "@thi.ng/paths": "^4.2.12",
        "@thi.ng/rstream": "^6.0.16",
        "@thi.ng/transducers": "^7.8.3",
        "@thi.ng/transducers-hdom": "^2.0.101",
        "@types/jest": "^27.0.1",
        "@types/node": "^16.6.1",
        "@typescript-eslint/eslint-plugin": "^4.29.2",
        "@typescript-eslint/parser": "^4.29.2",
        "babel-jest": "^27.0.6",
        "cross-env": "^7.0.3",
        "cssnano": "^5.0.7",
        "eslint": "^7.32.0",
        "eslint-plugin-jest": "^24.4.0",
        "gh-pages": "^3.2.3",
        "jest": "^27.0.6",
        "jest-environment-jsdom": "^27.0.6",
        "jsdom": "^17.0.0",
        "prettier": "^2.3.2",
        "ts-jest": "^27.0.4",
        "ts-node": "^10.2.0",
        "typescript": "^4.3.5"
    },
    "prettier": {
        "singleQuote": false,
        "printWidth": 100,
        "endOfLine": "lf",
        "tabWidth": 4,
        "semi": false,
        "arrowParens": "avoid"
    }
}
