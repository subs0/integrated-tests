{
  "name": "@-0/utils",
  "author": "Logan Powell",
  "license": "MIT",
  "version": "0.3.0",
  "description": "utilities for the `-0` org/framework built on @thi.ng/umbrella ecosystem",
  "main": "./lib/index.js",
  "type": "module",
  "types": "./lib/index.d.ts",
  "repository": "https://github.com/subs0/utils",
  "homepage": "https://github.com/subs0/utils",
  "scripts": {
    "madge": "madge --circular lib/",
    "typewatch": "tsc --project tsconfig.json",
    "types": "tsc --project tsconfig.build.json",
    "tests": "npm run types && npm run madge && jest",
    "patch": "npm version patch && npm publish",
    "postgit": "git push origin master && npm run patch",
    "git": "git add . && git commit -m"
  },
  "keywords": [
    "thi.ng",
    "-0",
    "FRP",
    "keys",
    "constants"
  ],
  "dependencies": {
    "querystring": "^0.2.1"
  },
  "peerDependencies": {
    "@-0/keys": "latest",
    "@thi.ng/rstream": "^5.1.0",
    "@thi.ng/checks": "latest"
  },
  "devDependencies": {
    "@-0/keys": "latest",
    "@thi.ng/rstream": "^5.1.0",
    "@thi.ng/checks": "latest",
    "@typescript-eslint/eslint-plugin": "^4.15.1",
    "@typescript-eslint/parser": "^4.15.1",
    "eslint": "^7.20.0",
    "jest": "^26.6.3",
    "prettier": "^2.2.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "prettier": {
    "singleQuote": false,
    "printWidth": 100,
    "endOfLine": "lf",
    "tabWidth": 2,
    "semi": false
  }
}
