{
    "name": "@-0/stack",
    "author": "Logan Powell",
    "license": "MIT",
    "version": "0.3.0",
    "description": "Monorepo for -0/ repo subtrees",
    "main": "./lib/index.js",
    "type": "module",
    "types": "./lib/index.d.ts",
    "repository": "https://github.com/subs0/stack",
    "homepage": "https://github.com/subs0/stack",
    "scripts": {
        "madge": "madge --circular lib/",
        "types": "tsc --project tsconfig.build.json",
        "typewatch": "tsc --project tsconfig.json",
        "test": "pnpm run --sequential madge && concurrently \"pnpm run typewatch\" \"jest --watchAll\"",
        "pages": "pnpm run --sequential build && gh-pages -d @-0/stack",
        "jsdoc": "jsdoc -c jsdoc/jsdoc.json --verbose --debug",
        "docs": "pnpm run --sequential types && jsdoc",
        "clean": "rm -rf .cache build dist",
        "build": "pnpm run --sequential clean && cross-env NODE_ENV=production parcel build example/index.html -d @-0/stack --no-source-maps --no-cache --public-url ./",
        "start": "pnpm run --sequential madge && concurrently \"pnpm run typewatch\" \"parcel example/index.html md -p 8080 --open -d dist watch\"",
        "postgit": "git push origin master",
        "git": "git add . && git commit -m",
        "cl:keys": "node scripts/clean st=keys",
        "cl:utils": "node scripts/clean st=utils",
        "cl:spool": "node scripts/clean st=spool",
        "cl:browser": "node scripts/clean st=browser",
        "tp:keys": "node scripts/types st=keys",
        "tp:spool": "node scripts/types st=spool",
        "tp:utils": "node scripts/types st=utils",
        "tp:browser": "node scripts/types st=browser",
        "pa:keys": "node scripts/patch st=keys",
        "pa:utils": "node scripts/patch st=utils",
        "pa:spool": "node scripts/patch st=spool",
        "pa:browser": "node scripts/patch st=browser",
        "pn:keys": "node scripts/prune st=keys",
        "pn:utils": "node scripts/prune st=utils",
        "pn:spool": "node scripts/prune st=spool",
        "pn:browser": "node scripts/prune st=browser",
        "up:keys": "node scripts/update st=keys msg=%pnpm_config_msg%",
        "up:utils": "node scripts/update st=utils msg=%pnpm_config_msg%",
        "up:spool": "node scripts/update st=spool msg=%pnpm_config_msg%",
        "up:browser": "node scripts/update st=browser msg=%pnpm_config_msg%",
        "go:keys": "pnpm run --sequential cl:keys && tp:keys && pa:keys && up:keys",
        "go:utils": "pnpm run --sequential cl:utils && tp:utils && pa:utils && up:utils ",
        "go:spool": "pnpm run --sequential cl:spool && tp:spool && pa:spool && up:spool",
        "go:browser": "pnpm run --sequential cl:browser && tp:browser && pa:browser && up:browser",
        "go:all": "pnpm run --sequential go:keys && go:utils && go:spool && go:browser && ncu",
        "st:keys": "node scripts/subtree st=keys via=push",
        "st:utils": "node scripts/subtree st=utils via=push",
        "st:spool": "node scripts/subtree st=spool via=push",
        "st:browser": "node scripts/subtree st=browser via=push",
        "update": "ncu -u",
        "ncu": "pnpm run --sequential --sequential update && pnpm i && pnpm audit fix",
        "check-env": "node -e 'console.log(process.env)' | grep pnpm",
        "example-pnpm-variable": "echo 'Notice how the variable is in CAPS %pnpm_config_msg% <- but not here!'",
        "go example": "pnpm run --sequential go:all MSG='some commit message'",
        "subtree:all": "pnpm run --sequential st:keys && st:utils && st:spool && st:browser",
        "prune:all": "pnpm run --sequential pn:keys && pn:utils && pn:spool && pn:browser"
    },
    "keywords": [
        "thi.ng",
        "multiplex",
        "FRP"
    ],
    "devDependencies": {
        "@-0/browser": "^0.3.86",
        "@-0/keys": "^0.3.73",
        "@-0/spool": "^0.3.57",
        "@-0/utils": "^0.3.67",
        "@babel/core": "^7.21.8",
        "@babel/plugin-transform-modules-commonjs": "^7.21.5",
        "@babel/preset-env": "^7.21.5",
        "@babel/preset-typescript": "^7.21.5",
        "@testing-library/dom": "^9.3.0",
        "@thi.ng/associative": "^6.2.37",
        "@thi.ng/atom": "^5.2.2",
        "@thi.ng/checks": "^3.3.13",
        "@thi.ng/paths": "^5.1.37",
        "@thi.ng/rstream": "^8.0.3",
        "@types/jest": "^29.5.1",
        "@types/node": "^20.1.4",
        "@typescript-eslint/eslint-plugin": "^5.59.5",
        "@typescript-eslint/parser": "^5.59.5",
        "babel-jest": "^29.5.0",
        "cssnano": "^6.0.1",
        "eslint": "^8.40.0",
        "eslint-plugin-jest": "^27.2.1",
        "gh-pages": "^5.0.0",
        "jest": "^29.5.0",
        "jest-environment-jsdom": "^29.5.0",
        "jsdom": "^22.0.0",
        "postcss": "^8.4.23",
        "prettier": "^2.8.8",
        "ts-node": "^10.9.1",
        "tslib": "^2.5.0",
        "typescript": "^5.0.4"
    },
    "prettier": {
        "singleQuote": false,
        "printWidth": 100,
        "endOfLine": "lf",
        "tabWidth": 4,
        "semi": false,
        "arrowParens": "avoid"
    }
}
