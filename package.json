{
  "name": "@-0/stack",
  "author": "Logan Powell",
  "license": "MIT",
  "version": "0.2.54",
  "description": "Monorepo for -0/ repo subtrees",
  "main": "./lib/index.js",
  "type": "module",
  "types": "./lib/index.d.ts",
  "repository": "https://github.com/subs0/stack",
  "homepage": "https://github.com/subs0/stack",
  "scripts": {
    "build": "npm run clean && cross-env NODE_ENV=production parcel build example/index.html -d @-0/stack --no-source-maps --no-cache --public-url ./",
    "start": "npm run types && parcel example/index.html --global md -p 8080 --open -d dist watch",
    "clean": "rm -rf .cache build dist",
    "madge": "madge --circular lib/",
    "typewatch": "tsc --project tsconfig.json",
    "types": "tsc --project tsconfig.build.json",
    "jsdoc": "jsdoc -c jsdoc/jsdoc.json --verbose --debug",
    "pages": "npm run build && gh-pages -d @-0/stack",
    "docs": "npm run types && npm run jsdoc",
    "tests": "npm run types && npm run madge && jest",
    "postgit": "git push origin master",
    "git": "git add . && git commit -m",
    "cl:keys": "node scripts/clean st=keys via=push",
    "cl:utils": "node scripts/clean st=utils via=push",
    "cl:spool": "node scripts/clean st=spool via=push",
    "cl:hdom": "node scripts/clean st=hdom via=push",
    "tp:keys": "node scripts/types st=keys",
    "tp:spool": "node scripts/types st=spool",
    "tp:utils": "node scripts/types st=utils",
    "tp:hdom": "node scripts/types st=hdom",
    "up:keys": "node scripts/update st=keys",
    "up:utils": "node scripts/update st=utils",
    "up:spool": "node scripts/update st=spool",
    "up:hdom": "node scripts/update st=hdom",
    "pa:keys": "node scripts/patch st=keys",
    "pa:utils": "node scripts/patch st=utils",
    "pa:spool": "node scripts/patch st=spool",
    "pa:hdom": "node scripts/patch st=hdom",
    "st:keys": "node scripts/subtree st=keys via=push",
    "st:utils": "node scripts/subtree st=utils via=push",
    "st:spool": "node scripts/subtree st=spool via=push",
    "st:hdom": "node scripts/subtree st=hdom via=push",
    "go:keys": "npm run cl:keys && npm run tp:keys && npm run up:keys && npm run st:keys && npm run ncu ",
    "go:utils": "npm run cl:utils && npm run tp:utils && npm run up:utils && npm run st:utils && npm run ncu ",
    "go:spool": "npm run cl:spool && npm run tp:spool && npm run up:spool && npm run st:spool && npm run ncu ",
    "go:hdom": "npm run cl:hdom && npm run tp:hdom && npm run up:hdom && npm run st:hdom && npm run ncu ",
    "go:all": "npm run go:keys && npm run go:utils && npm run go:spool && npm run go:hdom",
    "ncu": "ncu -u && npm i"
  },
  "keywords": [
    "thi.ng",
    "multiplex",
    "FRP"
  ],
  "peerDependencies": {
    "@thi.ng/arrays": "^0.5.4",
    "@thi.ng/associative": "3.1.6",
    "@thi.ng/atom": "^3.1.7",
    "@thi.ng/checks": "^2.5.3",
    "@thi.ng/paths": "^3.0.4",
    "@thi.ng/transducers": "^6.3.2",
    "@thi.ng/rstream": "^3.0.5",
    "@thi.ng/transducers-hdom": "^2.0.37"
  },
  "dependencies": {
    "@-0/hdom": "^0.2.68",
    "@-0/keys": "^0.2.66",
    "@-0/spool": "^0.2.67",
    "@-0/utils": "^0.2.59"
  },
  "devDependencies": {
    "@thi.ng/arrays": "^0.5.4",
    "@thi.ng/associative": "3.1.6",
    "@thi.ng/atom": "^3.1.7",
    "@thi.ng/checks": "^2.5.3",
    "@thi.ng/paths": "^3.0.4",
    "@thi.ng/rstream": "^3.0.5",
    "@thi.ng/transducers": "^6.3.2",
    "@thi.ng/transducers-hdom": "^2.0.37",
    "@typescript-eslint/eslint-plugin": "^2.21.0",
    "@typescript-eslint/parser": "^2.21.0",
    "better-docs": "^1.4.7",
    "cross-env": "^7.0.0",
    "cssnano": "^4.1.10",
    "eslint": "^6.8.0",
    "gh-pages": "^2.2.0",
    "jest": "^25.1.0",
    "prettier": "^1.19.1"
  },
  "prettier": {
    "singleQuote": false,
    "printWidth": 100,
    "endOfLine": "lf",
    "tabWidth": 2,
    "semi": false
  }
}
