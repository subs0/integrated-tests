{
    "env": {
        "browser": true,
        "es6": true,
        "jest": true,
        "node": true
    },
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "rules": {
        "react/prop-types": 0,
        "no-native-reassign": 0,
        "no-negated-in-lhs": 0,
        "no-unreachable": 1,
        "no-console": 0,
        "no-use-before-define": 0,
        "no-var": 0,
        "no-undef": 2,
        "prefer-const": 1
    },
    "extends": ["eslint:recommended"],
    "root": true,
    "plugins": ["jest"],
    "parserOptions": {
        "ecmaVersion": 11,
        "ecmaFeatures": {},
        "sourceType": "module"
    },
    "settings": {}
}
