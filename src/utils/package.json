    "license": "MIT",
    "version": "0.3.69",
    "description": "utilities for the `-0` org/framework built on @thi.ng/umbrella ecosystem",
    "main": "./src/index.js",
    "type": "module",
    "repository": "https://github.com/subs0/utils",
    "homepage": "https://github.com/subs0/utils",
    "scripts": {
        "madge": "madge --circular src/",
        "test": "pnpm run --sequential madge && NODE_OPTIONS=--experimental-vm-modules jest --watchAll",
        "patch": "npm version patch && npm publish",
        "push": "git push origin master",
        "update": "ncu -u",
        "ncu": "pnpm run --sequential update && pnpm i && pnpm audit fix",
        "git": "git add . && git commit -m"
    },
    "keywords": [
        "thi.ng",
        "-0",
        "FRP",
        "keys",
        "constants"
    ],
    "dependencies": {
        "@-0/keys": "link:/Users/logan/Documents/projects/@-0/stack/src/keys",
        "@thi.ng/checks": "^3.3.13",
        "@thi.ng/rstream": "^8.0.3"
    },
    "devDependencies": {
        "@babel/core": "^7.21.8",
        "@babel/plugin-transform-modules-commonjs": "^7.21.5",
        "@babel/preset-typescript": "^7.21.5",
        "@thi.ng/transducers": "^8.4.5",
        "eslint": "^8.40.0",
        "eslint-plugin-jest": "^27.2.1",
        "eslint-plugin-node": "^11.1.0",
        "jest": "^29.5.0",
        "prettier": "^2.8.8"
    },
    "publishConfig": {
        "access": "public"
    },
    "prettier": {
        "singleQuote": false,
        "printWidth": 120,
        "endOfLine": "lf",
        "tabWidth": 4,
        "semi": false,
        "arrowParens": "avoid"
    }
